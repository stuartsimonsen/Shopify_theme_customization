  
<div class="storelocator">
   <div class="page-width">
     <div class="grid page-width">     
        <div class="section-header text-left">
          <h1 class="pagecustomtitle">{{ page.title }}</h1>
          <h2 class="store_subtitle">Please fill out this form</h2>
        </div>
     </div>
   </div>
</div> 
 

<div class="page-width WebtoleadForm">
     <div class="page-width grid grid__item medium-up--two-thirds">
          {% if page.content.size > 0 %}
            <div class="rte">
              {{ page.content }}
            </div>
          {% endif %}

          <div class="contact-form form-vertical">

            <!--   <META HTTP-EQUIV="Content-type" CONTENT="text/html; charset=UTF-8"> -->
          
           
              <form action="https://webto.salesforce.com/servlet/servlet.WebToLead?encoding=UTF-8" method="POST">

              <input type=hidden name="oid" value="00D0O000000svq4">
              <input type=hidden name="retURL" value="https://pawtrails.com">

              <!--  ----------------------------------------------------------------------  -->
              <!--  NOTE: These fields are optional debugging elements. Please uncomment    -->
              <!--  these lines if you wish to test in debug mode.                          -->
              <input type="hidden" name="debug" value=1>                             
              <input type="hidden" name="debugEmail" value="ziyun@attitudetech.ie">   
              <!--  ----------------------------------------------------------------------  -->

              <label for="first_name">First Name</label><input  id="first_name" maxlength="40" name="first_name" size="20" type="text" /><br>

              <label for="last_name">Last Name</label><input  id="last_name" maxlength="80" name="last_name" size="20" type="text" /><br>

              <label for="email">Email</label><input  id="email" maxlength="80" name="email" size="20" type="text" /><br>

              <label for="company">Company</label><input  id="company" maxlength="40" name="company" size="20" type="text" /><br>

              <label for="city">City</label><input  id="city" maxlength="40" name="city" size="20" type="text" /><br>

              Lead Source:<select  id="00N0O00000EmXQw" name="00N0O00000EmXQw" title="Lead Source">
              <option value="">--None--</option><option value="Web">Web</option>
              <option value="Email">Email</option>
              <option value="Tradeshow">Tradeshow</option>
              <option value="Social media">Social media</option>
              <option value="Radio">Radio</option>
              <option value="Referral Partner">Referral Partner</option>
              <option value="Inbound Call">Inbound Call</option>
              <option value="Self-Generated">Self-Generated</option>
              </select><br>

              Description:<textarea  id="00N0O00000EmmXJ" name="00N0O00000EmmXJ" type="text" wrap="soft"></textarea><br>

              <input type="submit" name="submit">

              </form>



        
          </div>
      </div>
  
